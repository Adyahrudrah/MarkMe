{
  "manifest_version": 3,
  "name": "markMe",
  "version": "1.0",
  "description": "A high-performance bookmark manager that replaces your New Tab for better organization.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks", "storage"],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
