{
  "manifest_version": 3,
  "name": "markMe",
  "version": "1.0",
  "description": "A high-performance bookmark manager that replaces your New Tab for better organization.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks", "storage", "tabs"],
  "web_accessible_resources": [
    {
      "resources": ["dist/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
